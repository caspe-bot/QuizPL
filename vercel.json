{
  "version": 2,
  "builds": [
    { "src": "client/vite.config.ts", "use": "@vercel/static-build", "config": {} },
    { "src": "api/index.ts", "use": "@vercel/node" }
     ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.ts" },
    { "src": "/(.*)", "dest": "/client/dist/$1" }
     ],
  "outputDirectory": "client/dist"
  }
